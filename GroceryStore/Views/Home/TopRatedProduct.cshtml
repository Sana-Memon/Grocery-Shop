@model IEnumerable<GroceryStore.Models.product>

<section class="ftco-section ftco-no-pb">

    <div class="container">

        <div class="row">

            @foreach (var item in Model)
            {
                <div class="col-lg-2 col-md-4 ">
                    <div class="sort w-100 text-center ">
                        <div class="img" style="background-image: url(@item.ImageFilePath);"></div>
                        <h3>@item.product_name</h3>
                    </div>
                </div>

            }
        </div>
    </div>
</section>

